// from fopen 

import "std";

int main() {
  let file = io.fopen("code/test/file.aky", "r");
  char *myread = malloc(256L);
  print "%s\n", typeof(file);
  print "%s\n", typeof(myread);

  if !file {
    print "No file found\n";
    return 1;
  }

  let c = io.fgetc(file);
  print "%s\n", typeof(c);
  while c != EOF {
    print "%c", (char) c; 
    c = io.fgetc(file);
  }

  io.fclose(file);
  return 0;
}
