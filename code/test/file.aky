// from fopen 
import "std";

int main() {
  let file = io.fopen("code/test/file.aky", "r");
  char *myread = malloc(256 as long) as char*; // emang tolol builtin akowkawo
  print "Tipe file itu apa: %s\n", typeof(file);
  print "Tipe myread itu apa: %s\n", typeof(myread);

  if !file {
    print "No file found";
    return 1;
  }

  let c = io.fgetc(file);
  print "%s\n", typeof(c);
  while c != EOF {
    print "%c", c as char; 
    c = io.fgetc(file);
  }

  io.fclose(file);
  return 0;
}
