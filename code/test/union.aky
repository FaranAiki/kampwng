union Registration {
  int id;
  char *name;
  long idx;
};

int main() {
  printf "Testing union!\n";

  Registration jablay;

  jablay.id = 10;
  printf "10 == %d\n", jablay;

  jablay.name = c"bin"; // 4 
  printf "bin == %s\n", jablay;
  printf "randm == %d\n", jablay;

  jablay.idx = 1000000099999000L;
  printf "what is %ld\n", jablay;
  printf "what is %d\n", jablay;
}
