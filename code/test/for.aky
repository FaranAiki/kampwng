flux int gen(int max) {
  int t = 0;
  while ++t <= max {
    emit t;
  }
}

int main() {
  let t = gen(100);
  let my_arr = [1,2,3,4];
  print "%s\n", typeof(t);
  print "%s\n", typeof(my_arr);

  // print i to 500
  for i in 50 {
    print "%d\n", i;
  }

  // character looping
  print "This will loop hello world!\n";
  for c in "Hello, World!" {
    print "%c:%s\n", c, typeof(c);
  }

  // integer array looping 
  print "This will loop integer\n";
