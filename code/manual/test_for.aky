flux int gen() {
  int t = 0;
  while t <= 100 {
    t++;
    emit t;
  }
  return 1;
}

int main() {
  let t = gen();  
  print "%s\n", typeof(t);

  // print i to 500
  for i in 50 {
    print "%d\n", i;
  }

  print "This will loop hello world!\n";
  for c in "Hello, World!" {
    print "%c\n", c;
  }

  print "Emit!\n";
  for emitted in t {
    print "%d", emitted;
  }
}
